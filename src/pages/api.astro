---
import Navbar from "../layouts/Navbar.astro";
const res = await fetch('https://api.sampleapis.com/coffee/hot')
const data = await res.json()
---

<Navbar lang="en">
    <title>My Homepage</title>
</Navbar>
<ul>
    {data.map(item => (
      <li>{item.title} :</li>
      <p>{item.description}</p>
    ))}
  </ul>